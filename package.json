{
  "name": "@chubbyts/petstore",
  "version": "1.0.0",
  "description": "An api skeleton using mongodb for chubbyts-framework.",
  "keywords": [
    "chubbyts",
    "framework",
    "skleton",
    "api",
    "mongodb"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/petstore",
  "scripts": {
    "cs-fix": "prettier --write bin bootstrap config src tests *.js",
    "cs": "prettier --check bin bootstrap config src tests",
    "lint-fix": "eslint bin bootstrap config src tests --fix",
    "lint": "eslint bin bootstrap config src tests",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "infection": "stryker run",
    "build": "node build-cli.js",
    "build-watch": "node build-cli.js -w",
    "watch-server": "nodemon --watch \"dist/**/*\" dist/bootstrap/index.js",
    "start": "concurrently \"npm run build-watch\" \"sleep 0.1 && npm run watch-server\"",
    "command": "npm run build && node dist/bin/console.js"
  },
  "jest": {
    "transform": {
      "\\.ts$": "@swc/jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/unit/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100
      }
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^3.0.1",
    "@chubbyts/chubbyts-api": "^2.2.0",
    "@chubbyts/chubbyts-cors": "^1.0.3",
    "@chubbyts/chubbyts-decode-encode": "^1.1.1",
    "@chubbyts/chubbyts-dic": "^1.0.2",
    "@chubbyts/chubbyts-dic-config": "^1.0.2",
    "@chubbyts/chubbyts-dic-types": "^1.0.0",
    "@chubbyts/chubbyts-framework": "^1.6.3",
    "@chubbyts/chubbyts-framework-router-path-to-regexp": "^1.2.0",
    "@chubbyts/chubbyts-http": "^1.0.1",
    "@chubbyts/chubbyts-http-error": "^2.0.1",
    "@chubbyts/chubbyts-http-types": "^1.0.0",
    "@chubbyts/chubbyts-log-types": "^1.0.0",
    "@chubbyts/chubbyts-mongodb": "^1.0.1",
    "@chubbyts/chubbyts-negotiation": "^3.0.0",
    "@chubbyts/chubbyts-node-http-bridge": "^1.0.0",
    "@chubbyts/chubbyts-pino-adapter": "^1.1.1",
    "commander": "^9.4.1",
    "get-stream": "^6.0.1",
    "mongodb": "^4.12.0",
    "openapi3-ts": "^3.1.2",
    "pino": "^8.7.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.3",
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.3",
    "@types/node": "^14.18.33",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
